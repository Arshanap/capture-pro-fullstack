   <style>
    #searchbtn {
    background: none;
    border: none;
    padding: 0; 
    cursor: pointer; 
}
   .logout-btn {
            background-color: #dc3545; /* Bootstrap's danger color */
            color: white;
            border: none;
            border-radius: 5px;
            padding: 5px 10px; /* Smaller padding */
            font-size: 14px; /* Smaller font-size */
            display: inline-flex;
            align-items: center;
            cursor: pointer;
            width: 100%; /* Full width on small screens */
            max-width: 200px; /* Limit width on larger screens */
            justify-content: center;
            margin: 10px auto; /* Center the button */
        }

        .logout-btn i {
            margin-right: 0px; /* Adjust space between icon and text */
        }

        .logout-btn:hover {
            background-color: #c82333;
        }

        /* Responsive adjustments */
        @media (max-width: 768px) {
            .logout-btn {
                font-size: 12px;
                padding: 8px 12px;
            }
        }
    /* Base styles for all screen sizes */
.single-product {
    max-width: 90%; /* Default width for cards */
    margin: 0 auto;
}

.product-img img {
    max-height: 200px; /* Default image height */
    object-fit: contain;
}

.product-caption {
    padding: 10px;
    min-height: 150px;
}

.mb-60 {
    margin-bottom: 30px; /* Decrease space between cards */
}

/* Reduce size for large screens */
@media (min-width: 1200px) {
    .single-product {
        max-width: 80%; /* Further reduce width for larger screens */
    }
    .product-img img {
        max-height: 180px; /* Smaller image on large screens */
    }
    .product-caption {
        min-height: 130px; /* Reduce caption height */
    }
}

/* Adjust sizes for medium screens */
@media (min-width: 768px) and (max-width: 1199px) {
    .single-product {
        max-width: 95%; /* Slightly larger width for medium screens */
    }
    .product-img img {
        max-height: 220px; /* Slightly larger image for medium screens */
    }
    .product-caption {
        min-height: 160px;
    }
}

/* Adjust sizes for small screens */
@media (max-width: 767px) {
    .single-product {
        max-width: 100%; /* Full width for small screens */
    }
    .product-img img {
        max-height: 160px; /* Smaller images on small screens */
    }
    .product-caption {
        min-height: 140px;
    }
    .footer-toggle-btn {
        display: block !important;
    }
}
/* Styling the account icon button */
.header-btnw {
    display: inline-block;
    padding: 5px 5px;   /* Padding around the icon */
    /* background-color: #007bff;  */
    border-radius: 50%;  /* Makes the button circular */
    transition: background-color 0.3s ease, box-shadow 0.3s ease; /* Smooth hover transition */
    border: none; /* Removes border */
    text-align: center; /* Centers the icon */
    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1); /* Subtle shadow */
}

/* Styling the icon inside the button */
.header-btn i {
    font-size: 20px; /* Size of the icon */
    color: #fff; /* Icon color */
    vertical-align: middle; /* Centers the icon vertically */
}

/* Hover effect */
.header-btn:hover {
    /* background-color: #0056b3; Darken button background on hover */
    box-shadow: 0 6px 12px rgba(0, 0, 0, 0.2); /* Increase shadow on hover */
}

/* Smaller screens (responsive behavior for mobile) */
@media (max-width: 991px) {
    .header-btn {
        display: none; /* Hides the button on small screens */
    }
}

body, html {
    max-width: 100%;
    overflow-x: hidden;
}

.container {
    width: 100%;
    padding: 0 15px;
    margin: 0 auto;
}

/* Responsive adjustments */
@media (max-width: 576px) {
    .product-caption {
        padding: 5px;
        min-height: 120px;
    }

    .product-img img {
        max-height: 140px;
    }
}

@media (max-width: 991px) {
    .header-btn {
        display: none;
    }
}

/* Responsive banner images */
.banner img {
    width: 100%;
    height: auto;
}

/* Adjust banner image on smaller screens */
@media (max-width: 576px) {
    .banner img {
        max-height: 300px; /* Limit height on very small screens */
    }
}

/* Enhanced responsive footer styles for all screen sizes */
.footer-area {
    display: flex;
    flex-direction: column;
    align-items: center;
    text-align: center;
    padding: 20px 0;
    background-color: #f8f9fa;
}

.footer-toggle-btn {
    background-color: #007bff;
    color: white;
    border: none;
    border-radius: 5px;
    padding: 10px 20px;
    cursor: pointer;
    margin-bottom: 15px;
    transition: background-color 0.3s ease;
}

.footer-toggle-btn:hover {
    background-color: #0056b3;
}

.footer-area .container {
    display: flex;
    flex-direction: column;
    align-items: center;
    width: 100%;
    max-width: 1200px;
    padding: 0 15px;
}

.footer-area .row{
    display: flex;
    flex-direction: column;
    width: 100%;
    align-items: center;
    justify-content: center;
    margin-bottom: 20px;
}

.footer-area .col-xl-3, .footer-area .col-lg-3, .footer-area .col-md-5, .footer-area .col-sm-6 {
    width: 100%;
    margin-bottom: 20px;
    padding: 0 10px;
}

@media (min-width: 768px) {
    .footer-area .row {
        flex-direction: row;
        justify-content: space-between;
        margin-bottom: 0;
    }

    .footer-area .col-xl-3, .footer-area .col-lg-3, .footer-area .col-md-5, .footer-area .col-sm-6 {
        width: auto;
        margin-bottom: 0;
        flex: 1;
        max-width: 25%;
    }

    .footer-content {
        display: block !important;
    }
}

@media (min-width: 992px) {
    .footer-area .container {
        flex-direction: row;
        justify-content: space-between;
    }
}

@media (min-width: 1440px) {
    .footer-area .container {
        justify-content: space-evenly;
        padding: 0 30px;
    }

    .footer-area .col-xl-3, .footer-area .col-lg-3, .footer-area .col-md-5, .footer-area .col-sm-6 {
        padding: 0 20px;
        max-width: 23%;
    }
}
@media (max-width: 990px) {
    .navigation {
        display: none;
    }
    .mobile-menu {
        display: block;
    }
}
.row.align-items-center {
  margin-right: 0; /* Remove negative margin */
}

.container-fluid {
  padding-right: 20px; /* Increase parent padding */
}

.col-xl-6 {
  width: 70%; 
}
