    <script>
        function fundelete(event,url){
            event.preventDefault(); 

    Swal.fire({
        title: "Are you sure?",
        text: "You delete this Address!",
        showCancelButton: true,
        confirmButtonColor: "#3085d6",
        cancelButtonColor: "#d33",
        confirmButtonText: "Yes, delete it!"
    }).then((result) => {
        if (result.isConfirmed) {
            // Show success message and then redirect
            Swal.fire({
                title: "deleted!",
                text: "This Address has been deleted.",
                icon: "success",
                timer: 1500, // Display success message for 1.5 seconds
                showConfirmButton: false
            }).then(() => {
                // Redirect after success message is shown
                window.location.href = url;
            });
        }
    });
        }
        // JavaScript validation using Bootstrap's validation API
        (function () {
            'use strict';
            window.addEventListener('load', function () {
                // Get all forms with class 'needs-validation'
                var forms = document.getElementsByClassName('needs-validation');
                // Loop over them and prevent submission
                Array.prototype.filter.call(forms, function (form) {
                    form.addEventListener('submit', function (event) {
                        if (form.checkValidity() === false) {
                            event.preventDefault();
                            event.stopPropagation();
                        }
                        form.classList.add('was-validated');
                    }, false);
                });
            }, false);
        })();
    </script>
    <script>
