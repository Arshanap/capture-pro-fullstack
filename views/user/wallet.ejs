    <script>
        document.getElementById("walletForm").addEventListener("submit", function (event) {
  event.preventDefault();

  const amountInput = document.getElementById("amount");

  if (amountInput.value < 1 || amountInput.value === "") {
    amountInput.classList.add("is-invalid");
  } else {
    amountInput.classList.remove("is-invalid");

    const amount = amountInput.value;

    axios
      .post('/user/addFund', { amount: amount })
      .then(function (response) {
        Swal.fire({
          icon: 'success',
          title: 'Funds Added',
          text: 'Funds have been added successfully!',
          confirmButtonColor: '#3085d6',
        }).then(() => {
          // Reload the page after the user clicks "OK" on the success alert
          window.location.reload();
        });
      })
      .catch(function (error) {
        Swal.fire({
          icon: 'error',
          title: 'Error',
          text: 'Failed to add funds. Please try again.',
          confirmButtonColor: '#d33',
        });
      });
  }
});

      </script>
