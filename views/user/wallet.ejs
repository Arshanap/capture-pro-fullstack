            <!-- Recent Transactions -->
            <div style="width: 1100px; " class="card shadow-lg mt-4">
                <div class="card-header">
                  Recent Transactions
                </div>
                <div class="table-responsive">
                  <table class="table table-borderless mb-0">
                    <thead>
                      <tr>
                        <th scope="col">Transaction</th>
                        <th scope="col">Amount</th>
                        <th scope="col">Date</th>
                        <th scope="col">status</th>
                      </tr>
                    </thead>
                    <tbody>
                        <% wallet.transaction.forEach((item)=>{ %>
                      <tr class="transaction-item success">
                        <td>Payment Received</td>
                        <% if(item.transactionType === 'credit'){ %>
                        <td class="text-success">+₹  <%= item.amount %></td>
                        <% }else{ %>
                        <td class="text-danger">-₹  <%= item.amount %></td>
                        <% } %>
                        <td><small class="text-muted"><%= item.date.toDateString() %></small></td>
                        <td>
                            <p style="color: #4CAF50;"><%= item.status %></p>
                        </td>
                      </tr>
                      <% }) %>
                    </tbody>
                  </table>
                </div>
              </div>
              <div class="container m-3">
                <nav aria-label="Wallet navigation">
                    <ul class="pagination justify-content-center">
                
                        <% const totalPagesWithDefault = totalPages || 1; %> 
                        <% for (let i = 1; i <= totalPagesWithDefault; i++) { %>
                            <li class="page-item <%= (i === currentPage) ? 'active' : '' %>">
                                <a class="page-link" href="?page=<%= i %>"><%= i %></a>
                            </li>
                        <% } %>
                        
            
                       
                    </ul>
                </nav>
            </div>
            
              
          </div>
        
          <!-- Add Funds Modal -->
          <div class="modal fade" id="addFundsModal" tabindex="-1" aria-labelledby="addFundsModalLabel" aria-hidden="true">
            <div class="modal-dialog">
              <div class="modal-content">
                <div class="modal-header">
                  <h5 class="modal-title" id="addFundsModalLabel">Add Funds to Your Wallet</h5>
                  <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
                <div class="modal-body">
