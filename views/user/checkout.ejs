        <div class="container-content col-md-9" >
            <h2 class="text-center mb-4">Checkout</h2>
            <div class="row">
                <!-- Address Selection Table -->
                <div class="col-md-6 mb-4">
                    <button class="btn btn-secondary mb-3" data-toggle="modal" data-target="#addAddressModal" type="button">Add New Address</button>

                    <h4>Shipping Address</h4>
                    <br>
                    <table style="width: 700px;" class="table table-bordered mb-3">
                        <thead>
                            <tr>
                                <th>Address</th>
                                <th>Action</th>
                            </tr>
                        </thead>
                        <tbody id="addressTable">
                            <tr>
                                <td>123 Main St, New York, NY</td>
                                <td><button class="btn btn-secondary" onclick="editAddress('123 Main St, New York, NY')">Edit</button></td>
                            </tr>
                            <tr>
                                <td>456 Maple Ave, San Francisco, CA</td>
                                <td><button class="btn btn-secondary" onclick="editAddress('456 Maple Ave, San Francisco, CA')">Edit</button></td>
                            </tr>
                        </tbody>
                    </table>

                    <!-- New/Edit Address Form -->
                    <form id="addressForm" style="display:none;">
                        <h5 class="mt-3" id="addressFormTitle">Add or Edit Address</h5>
                        <button type="button" class="btn btn-primary" onclick="saveAddress()">Save Address</button>
                        <button type="button" class="btn btn-danger" onclick="deleteAddress()">Delete Address</button>
                    </form>
                </div>

                <!-- address add -->
                <div class="modal fade" id="addAddressModal" tabindex="-1" role="dialog" aria-labelledby="addAddressModalLabel" aria-hidden="true">
                    <div class="modal-dialog" role="document">
                        <div class="modal-content">
                            <div class="modal-header">
                                <h5 class="modal-title" id="addAddressModalLabel">Add New Address</h5>
                                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                    <span aria-hidden="true">&times;</span>
                                </button>
                            </div>
                            <div class="modal-body">
                                <form id="addressForm">
                                    <div class="form-group">
                                        <label for="fullName">Full Name</label>
                                        <input type="text" class="form-control" id="fullName" required>
                                    </div>
                                    <div class="form-group">
                                        <label for="pinCode">Pin Code</label>
                                        <input type="text" class="form-control" id="pinCode" required>
                                    </div>
                                    <div class="form-group">
                                        <label for="city">City</label>
                                        <input type="text" class="form-control" id="city" required>
                                    </div>
                                    <div class="form-group">
                                        <label for="state">State</label>
                                        <input type="text" class="form-control" id="state" required>
                                    </div>
                                    <div class="form-group">
                                        <label for="buildingName">Building Name</label>
                                        <input type="text" class="form-control" id="buildingName" required>
                                    </div>
                                    <div class="form-group">
                                        <label for="fullAddress">Enter your full Address</label>
                                        <textarea class="form-control" id="fullAddress" rows="3" required></textarea>
                                    </div>
                                </form>
                            </div>
                            <div class="modal-footer">
                                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                                <button type="submit" class="btn btn-primary" form="addressForm">Save Address</button>
                            </div>
                        </div>
                    </div>
                </div>
                

                <!-- Payment Method Selection -->
                <div style=" width: 300px; margin-left: 230px; margin-top: 60px;" class="col-md-6 mb-4">
                    <h4>Payment Method</h4><br>
                    <div class="mb-3">
                        <label>Select Payment Method:</label>
                        <div>
                            <input type="radio" id="creditCard" name="paymentMethod" value="creditCard" onchange="togglePaymentInfo('creditCard')">
                            <label for="creditCard">Credit Card</label>
                        </div>
                        
                        <div>
                            <input type="radio" id="paypal" name="paymentMethod" value="paypal" onchange="togglePaymentInfo('paypal')">
                            <label for="paypal">PayPal</label>
                        </div>
                        
                        <div>
                            <input type="radio" id="bankTransfer" name="paymentMethod" value="bankTransfer" onchange="togglePaymentInfo('bankTransfer')">
                            <label for="bankTransfer">Bank Transfer</label>
                        </div>
                    </div>

                    <!-- Payment Information Form -->
                    <div id="paymentInfo" style="display: none;">
                        <h5>Enter Payment Details</h5>
                        <form>
                            <div class="mb-3" id="creditCardInfo" style="display: none;">
                                <!-- Payment details fields here -->
                            </div>
                        </form>
                    </div>
                </div>
            </div>

            <!-- Cart Products List Table -->
            <h4 class="mb-4">Products</h4>
            <table class="table table-bordered">
                <thead>
                    <tr>
                        <th>Product</th>
                        <th>Quantity</th>
                        <th>Price</th>
                        <th>Total</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>Product 1</td>
                        <td>1</td>
                        <td>$50.00</td>
                        <td>$50.00</td>
                    </tr>
                    <tr>
                        <td>Product 2</td>
                        <td>1</td>
                        <td>$30.00</td>
                        <td>$30.00</td>
                    </tr>
                </tbody>
            </table>

            <!-- Order Summary Table -->
            <h4 class="mb-4">Order Summary</h4>
            <table  class="table table-bordered">
                <tbody>
                    <tr>
                        <td>Subtotal</td>
                        <td>$80.00</td>
                    </tr>
                    <tr>
                        <td>Shipping</td>
                        <td>$5.00</td>
                    </tr>
                    <tr class="fw-bold">
                        <td>Total</td>
                        <td>$85.00</td>
                    </tr>
                </tbody>
            </table>
            <button type="submit"  class="btn btn-primary w-100 py-2">Place Order</button>
        </div>
    </div>
